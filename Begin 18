#include <iostream>
using namespace std;

int main() {
    int A, B, C;
    cout <<"___begin 18___"<<endl;
    // Зчитуємо три цілі числа від користувача
    cout << "Введіть значення A: ";
    cin >> A;

    cout << "Введіть значення B: ";
    cin >> B;

    cout << "Введіть значення C: ";
    cin >> C;

    // Перевіряємо, чи є хоча б одна пара співпадаючих чисел
    if (A == B || B == C || A == C) {
        cout << "Є хоча б одна пара співпадаючих чисел.\n";
    } else {
        cout << "Усі три числа різні.\n";
    }

    return 0;
}
